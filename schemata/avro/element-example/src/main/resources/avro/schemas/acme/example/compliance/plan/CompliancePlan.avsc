    },
    {
      "name": "created_at",
      "type": "acme.common.time.Timestamp"
    },
    {
      "name": "entitlements",
      "type": {
        "type": "array",
        "items": "acme.example.compliance.plan.entitlement.Entitlement"
      }
    },
    {
      "name": "market",
      "type": "acme.example.market.Market"
    },
    {
      "name": "creator",
      "type": "acme.example.user.User"
    }
  ]
